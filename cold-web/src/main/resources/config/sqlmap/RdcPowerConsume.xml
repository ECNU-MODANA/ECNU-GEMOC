<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smartcold.manage.cold.dao.RdcPowerConsumeMapper">

    <select id="findLastNPoint" parameterType="map" resultType="RdcPowerConsumeEntity">
        SELECT * FROM `RdcPowerCosume` WHERE `rdcID` = #{rdcID} ORDER by 'addTime' DESC limit #{npoint}
    </select>

</mapper>